@inject IStringLocalizer<DeleteDialog> _localizer
<MudDialog>
    <DialogContent>
        <MudText>@ContentText</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">@_localizer["ButtonCancelText"]</MudButton>
        <MudButton Color="Color.Error" Variant="Variant.Filled" OnClick="Submit">@_localizer["ButtonConfirmText"]</MudButton>
    </DialogActions>
</MudDialog>
